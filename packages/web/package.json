{
  "name": "@camcloud/web",
  "version": "1.0.0",
  "private": true,
  "author": "Janek Rahrt <janek.rahrt@me.com>",
  "scripts": {
    "dev": "nodemon --inspect=9239 --watch server.js server.js",
    "build": "next build",
    "start": "node server.js",
    "format": "prettier --write --ignore-path .prettierignore \"**/*\"",
    "lint": "eslint '**/*.{js,jsx}'",
    "pretest": "yarn format && yarn lint",
    "test": "echo \"No test defined yet.\"",
    "docker-build": "docker build -t cam-web .",
    "docker-start": "docker run -p 3000:3000 cam-web",
    "unrefer": "yarn rerefer && mkdir .modules && cp -a node_modules/@camcloud .modules/ && yarn add file:./.modules/@camcloud/common/",
    "rerefer": "yarn add janek26/cam-common && rm -fr .modules",
    "deploy-dev": "now --public"
  },
  "dependencies": {
    "@camcloud/common": "janek26/cam-common#3f517f1efe3e3a7530ee65612d92bef03c9a59c5",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.1.20",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.9",
    "apollo-link-error": "^1.1.1",
    "apollo-link-ws": "^1.0.9",
    "apollo-utilities": "^1.0.24",
    "braintree-web-drop-in-react": "^1.0.5",
    "cookie": "^0.3.1",
    "evergreen-ui": "^4.2.0",
    "formik": "^1.3.1",
    "graphql": "^14.0.2",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^7.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-apollo": "^2.3.0",
    "react-dates": "^20.1.0",
    "react-dom": "^16.6.0",
    "react-measure": "^2.1.2",
    "react-paginate": "^6.3.0",
    "react-player": "^1.6.6",
    "react-powerplug": "^1.0.0-rc.1",
    "subscriptions-transport-ws": "^0.9.15",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-define": "^1.3.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.6",
    "now": "^11.4.6",
    "prettier": "1.14.3"
  }
}
