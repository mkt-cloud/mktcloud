{
  "name": "@camcloud/serverless-ffmpeg",
  "private": true,
  "author": "Janek Rahrt <janek.rahrt@me.com>",
  "version": "1.0.0",
  "description": "Run ffmpeg serverless!",
  "dependencies": {
    "aws-sdk": "^2.173.0",
    "axios": "^0.18.0",
    "monk": "^6.0.6"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "fs-extra": "^5.0.0",
    "serverless-s3-remover": "^0.4.1",
    "serverless-webpack": "^4.2.0",
    "webpack": "^3.10.0",
    "webpack-plugin-copy": "^1.0.1"
  },
  "scripts": {
